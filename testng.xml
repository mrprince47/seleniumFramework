<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="0">

  <listeners>
    <!-- Existing custom listener -->
    <listener class-name="listeners.ConfigFailureListener"/>
    
    <!-- ExtentReports listener -->
    <listener class-name="listeners.ExtentManager"/>
  </listeners>

  <!-- Browser parameter -->
  <parameter name="browser" value="chrome" />

  <test thread-count="5" name="Test" parallel="classes">
    <classes>
      <!--class name="testClasses.TC001_Registration"/-->
      <class name="testClasses.TC002_Login"/>
    </classes>
  </test>

</suite>
